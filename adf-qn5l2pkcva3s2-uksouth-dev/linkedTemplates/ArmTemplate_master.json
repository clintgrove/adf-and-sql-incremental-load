{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-qn5l2pkcva3s2-uksouth-dev"
		},
		"factstuff_DatabaseSOURCE_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'factstuff_DatabaseSOURCE'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=sqlsrv-th5dkllq2i3qa-dev.database.windows.net;Initial Catalog=dba-fac-stf-source"
		},
		"factstuff_DatabaseTARGET_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'factstuff_DatabaseTARGET'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=sqlsrv-th5dkllq2i3qa-dev.database.windows.net;Initial Catalog=dba-facstuff-target"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-qn5l2pkcva3s2-uksouth-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"factstuff_DatabaseSOURCE_connectionString": {
						"value": "[parameters('factstuff_DatabaseSOURCE_connectionString')]"
					},
					"factstuff_DatabaseTARGET_connectionString": {
						"value": "[parameters('factstuff_DatabaseTARGET_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}