{
	"name": "CDM dataflow 1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "source_datrans23_tblAuthors",
						"type": "DatasetReference"
					},
					"name": "sourceSQL"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "lakeStoreFactoryStuffAcc",
						"type": "LinkedServiceReference"
					},
					"schemaLinkedService": {
						"referenceName": "lakeStoreFactoryStuffAcc",
						"type": "LinkedServiceReference"
					},
					"name": "sinktoCDM"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          Id as integer,",
				"          Author_name as string,",
				"          country as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> sourceSQL",
				"sourceSQL sink(allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     input(",
				"          Id as string,",
				"          Author_name as string,",
				"          country as string",
				"     ),",
				"     entity: '/tblAuthors.cdm.json/tblAuthors',",
				"     format: 'cdm',",
				"     manifestType: 'manifest',",
				"     folderPath: 'azuresql',",
				"     fileSystem: 'dfact',",
				"     columnNamesAsHeader: false,",
				"     columnDelimiter: ',',",
				"     quoteChar: '\\\"',",
				"     escapeChar: '\\\\',",
				"     corpusPath: 'azuresql/schemaDocuments',",
				"     corpusStore: 'adlsgen2',",
				"     adlsgen2_fileSystem: 'dfact',",
				"     truncate: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          Id,",
				"          Author_name,",
				"          country",
				"     )) ~> sinktoCDM"
			]
		}
	}
}