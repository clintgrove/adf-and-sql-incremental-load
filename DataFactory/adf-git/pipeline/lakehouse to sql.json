{
	"name": "lakehouse to sql",
	"properties": {
		"activities": [
			{
				"name": "Copy dbr lakehouse to sql via staging",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureDatabricksDeltaLakeSource",
						"exportSettings": {
							"type": "AzureDatabricksDeltaLakeExportCommand"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": true,
					"stagingSettings": {
						"linkedServiceName": {
							"referenceName": "Gen2DataLakeStorage",
							"type": "LinkedServiceReference"
						},
						"path": "fssyntest"
					}
				},
				"inputs": [
					{
						"referenceName": "AzureDatabricksDeltaLakeDataset1",
						"type": "DatasetReference",
						"parameters": {
							"lakehouseSchema_param": "databaseclinto",
							"lakehouseTable_param": "mytaxiservice"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "dba_fac_a1_source",
						"type": "DatasetReference",
						"parameters": {
							"schema": "x",
							"tblname": "x"
						}
					}
				]
			}
		],
		"annotations": []
	}
}