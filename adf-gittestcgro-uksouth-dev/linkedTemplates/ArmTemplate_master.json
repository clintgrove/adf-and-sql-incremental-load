{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-gittestcgro-uksouth-dev"
		},
		"facstuff_DatabaseSOURCE_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'facstuff_DatabaseSOURCE'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=;Initial Catalog="
		},
		"facstuff_DatabaseTARGET_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'facstuff_DatabaseTARGET'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=;Initial Catalog="
		},
		"dataFactory_location": {
			"type": "string",
			"defaultValue": "uksouth"
		},
		"dataFactory_identity_type": {
			"type": "string",
			"defaultValue": "SystemAssigned"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-gittestcgro-uksouth-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"facstuff_DatabaseSOURCE_connectionString": {
						"value": "[parameters('facstuff_DatabaseSOURCE_connectionString')]"
					},
					"facstuff_DatabaseTARGET_connectionString": {
						"value": "[parameters('facstuff_DatabaseTARGET_connectionString')]"
					},
					"dataFactory_location": {
						"value": "[parameters('dataFactory_location')]"
					},
					"dataFactory_identity_type": {
						"value": "[parameters('dataFactory_identity_type')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}